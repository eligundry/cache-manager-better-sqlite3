{
  "name": "cache-manager-better-sqlite3",
  "version": "1.1.1",
  "description": "A modern SQLite store for node-cache-manager using better-sqlite3",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/*"
  ],
  "type": "module",
  "scripts": {
    "test": "node --loader ts-node/esm --test src/tests/*.ts",
    "build": "tsc -p tsconfig.build.json",
    "prettier": "prettier -w src",
    "prepublishOnly": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eligundry/cache-manager-better-sqlite3.git"
  },
  "keywords": [
    "cache-manager",
    "sqlite"
  ],
  "author": "Eli Gundry <eligundry@gmail.com>",
  "license": "MIT",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true
  },
  "dependencies": {
    "better-sqlite3": "^7.6.2",
    "cbor-x": "^1.3.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.2",
    "@types/sinon": "^10.0.13",
    "cache-manager": "^5.1.3",
    "prettier": "^2.7.1",
    "sinon": "^14.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "cache-manager": ">=5.1.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
